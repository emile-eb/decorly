#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules/.pnpm/react-native@0.74.7_@babel+_d92c6da321c1737794a3cd17076cd8af/node_modules/react-native/node_modules:/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules/.pnpm/react-native@0.74.7_@babel+_d92c6da321c1737794a3cd17076cd8af/node_modules:/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules/.pnpm/node_modules:/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules:/mnt/c/Users/Cheic/Decorly/apps/node_modules:/mnt/c/Users/Cheic/Decorly/node_modules:/mnt/c/Users/Cheic/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Cheic/decorly/apps/mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules/.pnpm/react-native@0.74.7_@babel+_d92c6da321c1737794a3cd17076cd8af/node_modules/react-native/node_modules:/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules/.pnpm/react-native@0.74.7_@babel+_d92c6da321c1737794a3cd17076cd8af/node_modules:/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules/.pnpm/node_modules:/mnt/c/Users/Cheic/Decorly/apps/mobile/node_modules:/mnt/c/Users/Cheic/Decorly/apps/node_modules:/mnt/c/Users/Cheic/Decorly/node_modules:/mnt/c/Users/Cheic/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Cheic/decorly/apps/mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../react-native/cli.js" "$@"
fi
